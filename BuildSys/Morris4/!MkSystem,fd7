wimpslot -min 5000k
echo Starting ...
time
Set MkSystem$Dir <Obey$Dir>
|
echo Exporting hdrs ...
dir <MkSystem$dir>
amu_machine export PHASE=hdrs
|
echo Exporting libs ...
dir <MkSystem$dir>
amu_machine export PHASE=libs
time
|
echo Copying Resource files to Messages module ...
dir <MkSystem$dir>
amu_machine resources
|
echo Building components ...
dir <MkSystem$dir>
amu_machine rom
|
echo Installing components ...
dir <MkSystem$dir>
echo <MkSystem$dir>
amu_machine install_rom 
|
echo All done.
time
